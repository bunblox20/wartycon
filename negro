--// Cargar la librería de Teleportation Master UI
local TeleportationUI = loadstring(game:GetObjects("rbxassetid://7657867786")[1].Source)()

--// Crear la ventana principal
local MainWindow = TeleportationUI:CreateWindow({
    Name = "Aimbot & Teleportation Master",
    Size = UDim2.new(0, 500, 0, 400), -- Tamaño de la ventana
    Themeable = {
        BackgroundColor = Color3.fromRGB(35, 35, 35),  -- Color de fondo de la interfaz
        TextColor = Color3.fromRGB(255, 255, 255),     -- Color del texto
        TitleColor = Color3.fromRGB(0, 191, 255)       -- Color del título
    }
})

--// Crear las pestañas
local TeleportTab = MainWindow:CreateTab({
    Name = "Teleportation"
})

local AimbotTab = MainWindow:CreateTab({
    Name = "Aimbot"
})

--// Teleportación (Pestaña)
local TeleportLocations = TeleportTab:CreateSection({
    Name = "Ubicaciones de Teleport"
})

-- Agregar botones para teletransportarse
TeleportLocations:AddButton({
    Name = "Instalación",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-21.13, -25.72, -71.03)
    end
})

TeleportLocations:AddButton({
    Name = "Torre de Radio",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2078.64, -7.47, 57.63)
    end
})

TeleportLocations:AddButton({
    Name = "Presa del Rio",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(22.34, 92.15, -1858.19)
    end
})

TeleportLocations:AddButton({
    Name = "Almacén",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2069.64, 29.92, 44.33)
    end
})

TeleportLocations:AddButton({
    Name = "Ciudad",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-152.41, 25.09, 2064.51)
    end
})

--// Aimbot (Pestaña)
local AimbotSettings = AimbotTab:CreateSection({
    Name = "Configuración del Aimbot"
})

-- Agregar controles para habilitar el Aimbot
AimbotSettings:AddToggle({
    Name = "Habilitar Aimbot",
    Value = true,
    Callback = function(Value)
        print("Aimbot habilitado: " .. tostring(Value))
    end
})

-- Agregar controles para configurar la sensibilidad
AimbotSettings:AddSlider({
    Name = "Sensibilidad",
    Value = 0.5,
    Min = 0,
    Max = 1,
    Callback = function(Value)
        print("Sensibilidad configurada: " .. tostring(Value))
    end
})

--// Función para cerrar la interfaz
MainWindow:CreateKeybind({
    Name = "Cerrar Interfaz",
    Callback = function()
        TeleportationUI:Unload()  -- Cerrar la interfaz
    end,
    Override = true,
    Options = {
        {KeyCode = Enum.KeyCode.LeftShift, Mode = Enum.KeyCode.LeftShift} -- Asignar una tecla para cerrar la interfaz
    }
})
